<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.webeclubbin.mynpr"
      android:versionCode="4" android:versionName="0.9.7">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="false" >
        <activity android:label="@string/app_name" android:name=".MyNPR">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    	<activity android:name="SearchStationTab"></activity>
    	<activity android:name="PopStoryTab"></activity>
    	<activity android:name="PlayListTab"></activity>

		<activity android:name="HTMLviewer"></activity>
		<service android:name="StreamingMediaPlayer"/>
</application>


<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-permission android:name="android.permission.RAISED_THREAD_PRIORITY"></uses-permission>
<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
<uses-sdk android:minSdkVersion="3"></uses-sdk>
</manifest> 